
@model IEnumerable<string>
  @Html.ActionLink("Home","List","Aircraft",null,new { @class = "btn btn-block btn-default btn-lg" + (null == ViewBag.SelectedCategory ? " btn-primary" : "") })
@foreach(var link in Model)
{
    if (link != null) { 
    @Html.RouteLink(link, new
    {
        Controller = "Aircraft",
        action = "List",
        category = link,
        page = 1
    }, new
    {

        @class = "btn btn-block btn-default btn-lg"
        + (link == ViewBag.SelectedCategory ? " btn-primary" : "")
    }
    );
    }
}
